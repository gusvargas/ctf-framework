<body>
  <h1>Login</h1>
  <form method="post">
    <div>
      <label for="team">Team</label>
      <input type="text" id="team" name="name">
    </div>
    <div>
      <label for="password">Password</label>
      <input type="password" id="password" name="password">
    </div>
    <div>
      <input type="submit" value="Login">
    </div>
  </form>
</body>
